{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "sourceMap": true,
    "target": "esnext",
    "newLine": "LF",
    "useDefineForClassFields": false,
    "declaration": true,
    "declarationMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": false,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": false,
    "lib": ["esnext", "dom"],
    "types": ["node"],
  },
  "include": [
    "./computedDataHandles/**/*",
    "./ActivityCall.ts",
    "./boolExpression.ts",
    "./index.ts",
    "./Controller.ts",
    "./InteractionCall.ts",
    "./MonoSystem.ts",
    "./System.ts",
    "./SQLite.ts",
    "./util.ts",

  ]
}
